{
    "compilerOptions": {
      "target": "es2022",
      "lib": ["es6"],
      "module": "CommonJS",
      "rootDir": "../cli-tool",
      "sourceMap": false,
      "outDir": "../build/cli-tool",
      "removeComments": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "strict": true,
      "skipLibCheck": true
    },
    "include": [
       "../cli-tool/**/*.ts"
    ]
  }
  